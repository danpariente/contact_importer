<h1>Contacts</h1>

<% contacts.each do |contact| %>
  <ul>
    <li><%= contact.name %></li>
    <li><%= contact.dob %></li>
    <li><%= contact.phone %></li>
    <li><%= contact.address %></li>
    <li><%= contact.credit_card %></li>
    <li><%= contact.franchise %></li>
    <li><%= contact.email %></li>
  </ul>
<% end %>
